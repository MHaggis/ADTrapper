# ADTrapper Environment Configuration
# Copy this file to .env and customize as needed

# Database password - use alphanumeric characters only (no +, =, /, etc.)
# These special chars can break the DATABASE_URL connection string
# Good: adtrapper_secure_2025
# Bad:  mO+J9WHI/rhBm=SI7Y (contains +, /, =)
POSTGRES_PASSWORD=adtrapper_dev_password

# Application URLs (defaults work for local development)
NEXT_PUBLIC_SUPABASE_URL=http://localhost:3001
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Node environment (development or production)
NODE_ENV=development

